{
  "name": "Today!",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Makes the \"Today\" button of Google Calendar more patriotic.",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [ "tabs", "https://calendar.google.com/" ],
  "web_accessible_resources": [ "NeilDiamondComingToAmerica.wav", "neil-diamond.jpg" ]
}
