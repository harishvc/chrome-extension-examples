{
  "manifest_version": 2,
  "name": "Google Calendar & Google Maps",
  "version": "0.1",
   "content_scripts": [
   { 
    "matches": ["*://calendar.google.com/calendar/render*"],
    "js": ["content.js"]
    }
   ],
   "background" : {
     "scripts" : ["background.js"],
     "persistent": false
   },
    "permissions": [
          "tabs"
  ]
}
