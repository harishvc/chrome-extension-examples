{
  "manifest_version": 2,
  "name": "MapIt for Google Calendar",
  "version": "1.4",
   "content_scripts": [
   { 
    "matches": ["*://calendar.google.com/calendar/render*"],
    "js": ["content.js"]
    }
   ],
   "background" : {
     "scripts" : ["background.js"],
     "persistent": false
   },
    "permissions": ["tabs","alarms"]
}
